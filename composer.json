{
    "name": "laravelplus/tenants",
    "description": "Multi-tenancy organization management package for Laravel.",
    "keywords": ["laravel", "tenants", "organizations", "multi-tenancy"],
    "license": "MIT",
    "authors": [
        {
            "name": "LaravelPlus",
            "email": "info@laravelplus.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/mail": "^12.0",
        "illuminate/notifications": "^12.0"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^2.0",
        "laravelplus/global-settings": "@dev",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-permission": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelPlus\\Tenants\\": "src/",
            "LaravelPlus\\Tenants\\Database\\Factories\\": "database/factories/",
            "LaravelPlus\\Tenants\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelPlus\\Tenants\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPlus\\Tenants\\TenantsServiceProvider"
            ],
            "aliases": {
                "Organization": "LaravelPlus\\Tenants\\Facades\\Organization"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../global-settings"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
